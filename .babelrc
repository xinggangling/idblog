{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        [
            "@babel/transform-runtime",
            {
              "absoluteRuntime": false,
              "corejs": false,
              "helpers": false,
              "regenerator": true,
              "useESModules": false
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        "@babel/plugin-transform-async-to-generator"
    ]
}
